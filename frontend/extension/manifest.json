{
    "name": "trst",
    "version": "0.1",
    "description": "trst real story tester",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
      },
    "content_scripts": [
        {
            "matches": [
            "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "permissions": ["declarativeContent", "activeTab", "storage"],
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "32": "rsc/img/logo.png"
        },
        "default_title": "trst"
    },
    "icons": {
        "32": "rsc/img/logo.png"
    },
    "options_page": "options.html",
    "manifest_version": 2
}